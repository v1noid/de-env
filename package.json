{
  "name": "de-env",
  "description": "A command-line tool for managing environment variables",
  "type": "module",
  "version": "1.1.0",
  "author": {
    "name": "v1noid",
    "email": "contact@v1noid.com",
    "url": "https://github.com/v1noid"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/v1noid/de-env"
  },
  "homepage": "https://github.com/v1noid/de-env#readme",
  "bugs": {
    "url": "https://github.com/v1noid/de-env/issues"
  },
  "keywords": [
    "env",
    "environment",
    "variables",
    "cli",
    "command-line"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "bun run --watch index.ts",
    "build": "tsup"
  },
  "bin": {
    "de-env": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "ts-morph": "^25.0.1"
  }
}
